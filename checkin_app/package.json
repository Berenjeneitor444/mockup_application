{
  "name": "checkin",
  "version": "1.7.4-rc.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0",
    "start:prod": "ng build --configuration=production && npx http-server dist/app",
    "start:staging": "ng build --configuration=staging && npx http-server dist/app",
    "build": "ng build",
    "build:staging": "ng build --configuration=staging",
    "dist": "./scripts/dist.sh",
    "watch": "ng build --watch --configuration=development",
    "test": "ng test",
